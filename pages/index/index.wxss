/* 卡盟板块 */
.kamengboard {
	background: #fff;
	box-shadow: 0 12rpx 42rpx -18rpx rgba(44, 71, 148, 1);
	border-radius: 32rpx;
	overflow: hidden;
}

.kamengboard text {
	color: #666;
}

.kameng-head,
.kameng1,
.kameng-title {
	text-align: center;
}

.kameng-title {
	margin-bottom: 30rpx;
}

.kameng-head text {
	height: 88rpx;
	background: #7661D1;
	color: #fff;
	line-height: 88rpx;
}

.kameng-head image {
	width: 356rpx;
	height: 74rpx;
	margin: 80rpx auto 20rpx;
}

.kameng1 .kmtitle {
	margin: 50rpx auto;
}

.kameng1 text {
	font-size: 24rpx;
	padding: 5rpx;
}

.kameng1 image {
	width: 516rpx;
	height: 166rpx;
	margin: 80rpx auto 60rpx;
}

.kameng2 image {
	width: 710rpx;
	height: 380rpx;
	margin: 0 auto 30rpx;
}

.km2-point {
	padding: 0 50rpx;
}

.km2-point text {
	font-size: 18rpx;
	color: #7661d1;
	padding: 5rpx 0;
}

.km2-point .row view {
	width: 150rpx;
	margin-right: 96rpx;
	margin: 0 96rpx 50rpx 0;
}

.km2-point .row view .greytext {
	color: #9a9a9a;
}

.km2-point .kmh3 {
	color: #333;
	padding-bottom: 5rpx;
}

.km2-point .line1 {
	width: 140rpx;
	height: 2px;
	color: #333;
	background: #7661d1;
	border-radius: 4px;
	margin-bottom: 5rpx;
}

.km-kk {
	flex-wrap: wrap;
}

.km-kk view {
	display: flex;
	flex-direction: column;
	width: 50%;
	height: 432rpx;
	justify-content: center;
	align-items: center;
}

.kkimg {
	background: #eeeeee;
}

.kkimg image {
	width: 228rpx;
	height: 404rpx;
}

.km-kk text {
	color: #333;
}

.km-kk .kmh3 {
	color: #666;
}

.tc-table {
	position: relative;

}

.tc-name {
	width: 100%;
	text-align: center;
	position: absolute;
	top: 6rpx;
}

.tc-table text {
	display: inline-block;
}

.tc-table .line2 {
	width: 100%;
	height: 2px;
	background: #a4a4a4;
}

.tc-name .kmh2 {
	padding: 0 20rpx;
	background: #fff;
	z-index: 1000;
}

.zifei {
	margin: 30rpx 0;
}

.zifei view {
	flex: 1;
	text-align: center;
}

.zifei .kmh3 {
	width: 100%;
}

.zifei .line3 {
	border-right: 2px solid #7661d1;
}

.zftable {
	padding: 20rpx 20rpx 40rpx;
}

.zftable view text {
	width: 100%;
	flex: 1;
	text-align: center;
	align-items: center;
}

.zftable view .flex2 {
	flex: 2;
}

.zftable view .flex1 {
	flex: 1;
}

.zftable view .flex3 {
	flex: 3;
}

.zftable view .flex4 {
	flex: 4;
}

.zftable .th text {
	font-weight: bold;
}

.zftable .th text,
.zftable .td text {
	padding: 20rpx 10rpx;
	background: #eee;
	border: 2px solid #fff;
	font-size: 20rpx;
	box-sizing: border-box;
}

.kameng5 .row image {
	width: 312rpx;
	height: 556rpx;
}

.kameng5 .row view {
	flex: 1;
	padding: 0 20rpx;
	box-sizing: border-box;
}

.kameng5 .row view text {
	font-size: 24rpx;
	color: #666;
	line-height: 55rpx;
}

.kameng5 .product {
	background: #eee;
	text-align: center;
	margin-top: 80rpx;
	padding-bottom: 70rpx;
}

.kameng5 .product text {
	padding: 50rpx;
}

.kameng5 .product image {
	width: 620rpx;
	height: 540rpx;
	margin: 0 auto;
}

.kameng-foot {
	width: 100%;
	height: 96rpx;
	font-size: 36rpx;
	background: #7661D1;
	text-align: center;
	line-height: 96rpx;
	color: #fff;
}

.kamengboard .purple {
	color: #7660d5;
}

.kamengboard .kmtitle {
	font-size: 70rpx;
	font-weight: bold;
	margin: 10rpx auto;
	color: #313131;
}

.kamengboard .kmh2 {
	font-size: 36rpx;
}

.kamengboard .line0 {
	display: block;
	width: 50rpx;
	height: 3px;
	background: #acacac;
	border-radius: 6px;
	margin: 20rpx auto;
}

.kamengboard .kmh4 {
	font-size: 20rpx;
	color: #acacac;
}

.kamengboard .kmh3 {
	font-size: 28rpx;
}

/* 会员中心板块 */
.memberboard .weallists .row {
	width: 100%;
	height: 160rpx;
	background-color: #fff;
	box-shadow: 0rpx 12rpx 42rpx -18rpx #2c4794;
	border-radius: 32rpx;
	margin-bottom: 20rpx;
	
	font-size: 36rpx;
	box-sizing: border-box;
}
.memberboard .weallists .column{
	justify-content: center;
}
.memberboard .weallists .memdetails text{
	color: #4B4B4B;
	font-size: 26rpx;
	line-height: 40rpx;
}
.memtitle{
	font-size: 30rpx;
	font-weight: bold;
}
.memberboard .weallists .column .memprice{
	color: #917ddc;
}
.memimg image{
	width: 120rpx;
	height: 120rpx;
}
.memimg, .membuy{
	flex: 1;
	align-items: center;
}
.memdetails{
	flex: 2;
}
.membuy text{
	font-size: 28rpx;
	background: #917ddc;
    color: #FFFFFF;
    padding: 10rpx 40rpx;
    border-radius: 40rpx;
}
.memberboard .moreweal {
	text-align: center;
	padding: 30rpx 0;
}
.memberboard .moreweal text{
	font-size: 28rpx;
	color: #cccccc;
}
.goTop {
	height: 80rpx;
	width: 80rpx;
	position: fixed;
	bottom: 50rpx;
	right: 30rpx;
}
/* 遮罩层 */
.maskLayer {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: #000;
	opacity: 0.2;
	overflow: hidden;
	z-index: 1000;
	color: #fff;
}

/*抽屉弹框 */
.bottom_box {
	width: 100%;
	display: flex;
	flex-direction: column;
	overflow: hidden;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 2000;
	background: #fff;
	box-sizing: border-box;
	border-radius: 32rpx 32rpx 0rpx 0rpx;
}

.bottom_box_con {
	display: flex;
	flex-direction: column;
	padding: 10rpx 10rpx;
	margin-bottom: 120rpx;
}
.closevip image{
	width: 50rpx;
	height: 50rpx;
	padding: 20rpx 20rpx 30rpx;
	float: right;
}
.wenxintips{
	text-align: center;
	clear: both;
	color: #4B4B4B;
	font-size: 32rpx;
	margin: 0 0 80rpx;
}
.wenxintips, .vipinput input{
	display: block;
	font-size: 32rpx;
}
.vipinput {
	width: 75%;
	margin: 10rpx auto 80rpx;
}
.buyvip{
	width: 100%;
	height: 96rpx;
	background: #917ddc;
	text-align: center;
	line-height: 96rpx;
}
.buyvip text{
	font-size: 32rpx;
	color: #fff;
}
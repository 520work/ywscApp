/**index.wxss**/
@import "../templete/model/model.wxss";
page {
	background-color: #f4f4f4;
	font-family: PingFangSC-Medium;
}

/* 首页轮播 begin */
.focusWrap .swiperCon {
	width: 100%;
	height: 330rpx;
}

.focusWrap .swiperCon swiper-item image {
	width: 100%;
	height: 260rpx;
}

/* 滑块导航 begin */
.tabWrap {
	margin-top: -137rpx;
}
.tabWrap .scrollCon .scroll-view-x {
	width: 1220rpx;
	height: 370rpx;
	display: flex;
	align-items: center;
}
.scrollpadding0{
	padding-left: 20rpx;
}
.scrollpadding1, .scrollpadding2{
	padding-left: 10rpx;
}
.scrollpadding3{
	padding-left: 10rpx;
	padding-right: 20rpx;
}
.tabWrap .scrollCon .scroll-view-x .selected, .tabWrap .scrollCon .scroll-view-x  .default {
	background-color: #fff;
}

.tabWrap .scrollCon .scroll-view-x .default {
	width: 286rpx;
	height: 288rpx;
	box-sizing: border-box;
	display: table;
	padding-top: 30rpx;
	padding-left: 28rpx;
	border-radius: 28rpx;
}

.tabWrap .scrollCon .scroll-view-x .default text {
	display: block;
}

.tabWrap .scrollCon .scroll-view-x .default .itemtitle {
	font-size: 30rpx;
	font-weight: 600;
	color: #999;
}

.tabWrap .scrollCon .scroll-view-x .default .itemsummary {
	height: 60rpx;
	font-weight: 500;
	font-size: 22rpx;
	color: #666;
	line-height: 30rpx;
	margin-top: 30rpx;
}

.tabWrap .scrollCon .scroll-view-x .selected {
	width: 300rpx;
	height: 310rpx;
	display: table;
	box-sizing: border-box;
	padding-top: 36rpx;
	padding-left: 34rpx;
	border-radius: 32rpx;
	box-shadow: 0rpx 12rpx 30rpx -18rpx;
}

.tabWrap .scrollCon .scroll-view-x .selected text {
	display: block;
}

.tabWrap .scrollCon .scroll-view-x .selected .itemtitle {
	font-size: 34rpx;
	font-weight: 600;
	color: #917DDC;
	line-height: 28px;
}

.tabWrap .scrollCon .scroll-view-x .selected .itemsummary {
	height: 68rpx;
	line-height: 34rpx;
	font-size: 24rpx;
	font-weight: 500;
	color: #666;
	margin-top: 30rpx;
}

.selectImg {
	width: 80rpx;
	height: 65rpx;
	margin-top: 30rpx;
}

.selectImg1 {
	width: 70rpx;
	height: 65rpx;
	margin-top: 20rpx;
}

.biger {
	-webkit-transform: scale(1.1);
}

.small {
	-webkit-transform: scale(0.9);
}

.show {
	display: block;
}

.hidden {
	display: none;
}

/* 首页切换面板 */
.goukaboard,
.temaiBoard,
.kamengboard,
.memberboard {
	width: 710rpx;
	margin: 0rpx auto;
	position: relative;

}

.mb {
	height: 32rpx;
}

/* 购卡板块 */
.goukaboard .goukason {
	height: 180rpx;
	box-shadow: 0rpx 12rpx 42rpx -18rpx rgba(44, 71, 148, 1);
	border-radius: 32rpx;
	margin: 0rpx auto 20rpx;
	box-sizing: border-box;
	padding: 32rpx 0 0 32rpx;
	position: relative;
}

.goukaboard .goukason text {
	display: block;
	color: #fff;
}

.goukaboard .goukason image {
	height: 100%;
	border-radius: 32rpx;
	position: absolute;
	top: 0;
	right: 0;
}

.goukaboard .goukason3 image {
	width: 270rpx;
}

.goukaboard .goukason2 image {
	width: 216rpx;
}

.goukaboard .goukason1 image {
	width: 210rpx;
}

.goukaboard .goukason4 image {
	width: 264rpx;
}

.goukaboard .goukason .title {
	font-size: 32rpx;
	margin-bottom: 16rpx;
	font-weight: 600;
}

.goukaboard .goukason .description {
	font-size: 28rpx;
}

.goukaboard .goukason1 {
	background: linear-gradient(to right, #2956ec, #4d75ed);
}

.goukaboard .goukason2 {
	background: linear-gradient(to right, #ff9514, #ffbb6c);
}

.goukaboard .goukason3 {
	background: linear-gradient(to right, #20b882, #41d69c);
}

.goukaboard .goukason4 {
	background: linear-gradient(to right, #f94a4e, #fe715a);
}

/* 特卖板块 */
.temaiBoard {
	background: rgba(255, 255, 255, 1);
	box-shadow: 0 12rpx 42rpx -18rpx rgba(44, 71, 148, 1);
	border-radius: 32rpx;
}

.temaiBoard .temaiTab {
	width: 100%;
	height: 114rpx;
	display: flex;
	flex-direction: row;
	padding: 36rpx 0 0 0;
}

.temaiBoard .temaiTab .title {
	display: inline-block;
	width: 192rpx;
}

.temaiBoard .temaiTab .line {
	width: 0px;
	height: 26rpx;
	border: 1px solid #A5A5A5;
	margin-top: 15rpx;
}

.temaiBoard .temaiTab view {
	flex: 1;
	text-align: center;
}

.temaiBoard .temaiTab .temaidefault .title {
	color: #666666;
	font-size: 32rpx;
	letter-spacing: 2rpx;
	font-weight: 400;
}

.temaiBoard .temaiTab .temaiselected .title {
	color: #7661D1;
	font-size: 32rpx;
	letter-spacing: 2rpx;
	border-bottom: #7661D1 solid 6rpx;
	padding-bottom: 15rpx;
}

.temaiBoard .lianghaoboard {
	width: 100%;
	display: flex;
	flex-wrap: wrap;
}

.temaiBoard .lianghaoboard .lianghaolists {
	width: 50%;
	display: inline-block;
	margin-bottom: 26rpx;
}

.temaiBoard .lianghaoboard .lianghaolists .top,
.temaiBoard .lianghaoboard .lianghaolists .bottom {
	height: 38rpx;
	padding: 0 32rpx;
}

.temaiBoard .lianghaoboard .lianghaolists .bottom view text {
	margin-left: 10rpx;
}

.temaiBoard .lianghaoboard .lianghaolists .top .icon-xinheart256 {
	color: #FF8C8C;
}

.temaiBoard .lianghaoboard .lianghaolists .tel {
	flex: 9;
	font-size: 28rpx;
	font-weight: 500;
	color: #666;
	letter-spacing: 2rpx;
}

.temaiBoard .lianghaoboard .lianghaolists .liangicon {
	width: 26rpx;
	height: 26rpx;
	font-size: 18rpx;
	color: #fff;
	text-align: center;
	flex: 1;
	vertical-align: middle;
	background: #E65656;
	border-radius: 4px;
}

.temaiBoard .lianghaoboard .lianghaolists .wherefrom {
	flex: 2;
	font-size: 28rpx;
	font-weight: 400;
	color: #666;
	height: 40rpx;
	line-height: 40rpx;
	letter-spacing: 2rpx;
}

.temaiBoard .lianghaoboard .lianghaolists .op {
	flex: 1;
	text-decoration: line-through;
	font-size: 24rpx;
	color: #999;
}

.temaiBoard .lianghaoboard .lianghaolists .np {
	flex: 1;
	font-size: 28rpx;
	color: #7661D1;
}

.temaiBoard .moretelnum {
	text-align: center;
	font-size: 28rpx;
	font-weight: 400;
	color: #7661D1;
	text-decoration: underline;
	padding: 20rpx 0 54rpx 0;
	letter-spacing: 2rpx;
}

.temaiBoard .lovetelboard .lovetellists {
	height: 104rpx;
	display: flex;
	padding-left: 42rpx;
	margin: 18rpx 0;
}

.temaiBoard .lovetelboard .lovetellists .lovetll {
	flex: 2;
	height: 100%;
}

.temaiBoard .lovetelboard .lovetellists .lovetll view {
	display: block;
	font-size: 32rpx;
	color: #666;
}

.temaiBoard .lovetelboard .lovetellists .lovetll .lovenum {
	color: #FF8C8C;
}

.temaiBoard .lovetelboard .lovetellists .lovetlm {
	flex: 1;

}

.temaiBoard .lovetelboard .lovetellists .lovetlm text {
	line-height: 92rpx;
	font-size: 28rpx;
	font-weight: 400;
	color: #999;
	padding-left: 20rpx;
}

.temaiBoard .lovetelboard .lovetellists .lovetlr {
	flex: 2;
	text-align: right;
	line-height: 92rpx;
}

.temaiBoard .lovetelboard .lovetellists .lovetlr .op {
	font-size: 24rpx;
	color: #999;
	text-decoration: line-through;
	margin-right: 10rpx;
}

.temaiBoard .lovetelboard .lovetellists .lovetlr .np {
	font-size: 28rpx;
	color: #7661D1;
	padding-right: 40rpx;
}

.temaiBoard .wholelove {
	padding-bottom: 44rpx;
}
.numbertype{
	width: 26rpx;
	height: 26rpx;
}

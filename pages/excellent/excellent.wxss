/* pages/excellent/excellent.wxss */
.excellent-container{
	padding-bottom: 60rpx;
}
.choosetaocan {
	justify-content: center;
	margin: 30rpx auto;
}

.choosetaocan .first,
.choosetaocan .second {
	width: 332rpx;
	height: 140rpx;
	border-radius: 32rpx;
	font-size: 26rpx;
	color: #fff;
	text-align: center;
	padding-top: 28rpx;
	box-sizing: border-box;
}

.choosetaocan .first {
	margin-right: 20rpx;
}

.choosetaocan view {
	background-color: #9d90d7;
}
.choosetaocan .active{
	background-color: #7661d1;
}

.choosetaocan .biger {
	font-size: 36rpx;
}

.introduce {
	width: 684rpx;
	margin: 0 auto;
	background: rgba(255, 255, 255, 1);
	box-shadow: 0rpx 12rpx 42rpx -18rpx rgba(44, 71, 148, 1);
	border-radius: 32rpx;
	padding-top: 50rpx;
}
.tcbh, .twzf{
	font-size: 28rpx;
	color: #7661D1;
	font-weight: bold;
	text-align: center;
}
.tcbh .title, .twzf .title{
	padding-bottom: 30rpx;
}
.tcbh .row, .twzf .row{
	color: #333333;
	font-weight: normal;
	margin-bottom: 60rpx;
}
.tcbh .row view, .twzf .row view{
	flex: 1;
}
.tcbh .row .bottom, .twzf .row .bottom{
	color: #666;
}
.zfsm{
	padding: 0 40rpx 30rpx;
	font-size: 20rpx;
	color: #666666;
}
.zfsm text{
	line-height: 30rpx;
	padding: 10rpx 0;
}
.gotochoosnum{
	height: 98rpx;
	background-color: #7661D1;
	font-size: 36rpx;
	color: #fff;
	border-bottom-left-radius: 32rpx;
	border-bottom-right-radius: 32rpx;
	text-align: center;
	line-height: 98rpx;
}
.icon-youjiantou:after{
	padding-left: 10rpx;
}